        android:supportsRtl="true"
        android:theme="@style/Theme.Bazas">
        <activity
            android:name=".loadGame.Activity_Load_Game"
            android:exported="false" />
        <activity android:name=".game.Activity_Game" />
        <activity android:name=".gameSettings.Activity_GameSettings" />
        <activity android:name=".Activity_Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>